<%= link_to 'New Employee Contract', new_employee_path %>
<table>
  <tr>
    <th>Contract</th>
	<th colspan="3"></th>
  </tr>
 
  <% i = 1 %>
  <% @employees.each do |employee| %>
    <tr>
      <td><%= link_to employee.contractPickFromDropDown, employee %></td>
	  <td><%= link_to 'Show', employee_path(employee) %></td>
	  <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
	  <td><%= link_to 'Destroy', employee_path(employee),
                    method: :delete, data: { confirm: 'Are you sure?' }%></td>
    </tr>
  <% i += 1 %>
  <% end %>
</table>