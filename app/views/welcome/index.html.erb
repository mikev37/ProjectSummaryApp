<h1>Project Manager Rails App</h1>
<%= link_to 'Employee Contracts', controller: 'employees' %>
<%= link_to 'New employee contract', new_employee_path %>
<h1>Sign In</h1>
<%= form_for :user, :url => sign_in_path(@user) do |f| %>
  <p>
    <%= f.label 'username:' %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label 'password:' %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Sign In' %>
  </p>
<% end %>